{
    "name": "wulfheart/laravel-actions-ide-helper",
    "description": "Generate a new IDE Helper file for Laravel Actions.",
    "keywords": [
        "laravel",
        "laravel-actions-ide-helper"
    ],
    "homepage": "https://github.com/fmtod/laravel-actions-ide-helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor R",
            "email": "it@fmtod.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "lorisleiva/laravel-actions": "^2.5",
        "lorisleiva/lody": "^0.4",
        "phpdocumentor/reflection": "^5.3",
        "ridzhi/docbuilder": "^1.0",
        "spatie/laravel-package-tools": "^1.14.1"
    },
    "require-dev": {
        "brianium/paratest": "^6.4",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "phpunit/phpunit": "^9.5.10",
        "spatie/invade": "^1.0",
        "spatie/laravel-ray": "^1.29",
        "spatie/pest-plugin-snapshots": "^1.1",
        "vimeo/psalm": "^4.20"
    },
    "autoload": {
        "psr-4": {
            "FmTod\\IdeHelperActions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FmTod\\IdeHelperActions\\Tests\\": "tests"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FmTod\\IdeHelperActions\\LaravelActionsIdeHelperServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
